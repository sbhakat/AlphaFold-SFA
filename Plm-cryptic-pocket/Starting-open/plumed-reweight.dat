tic0:   READ FILE=COLVAR  IGNORE_TIME VALUES=sf1
tic1:   READ FILE=COLVAR  IGNORE_TIME VALUES=sf2

chi1:   READ FILE=COLVAR  IGNORE_TIME VALUES=chi1
chi2:   READ FILE=COLVAR  IGNORE_TIME VALUES=chi2
d1:     READ FILE=COLVAR  IGNORE_TIME VALUES=d1
d3:     READ FILE=COLVAR  IGNORE_TIME VALUES=d3
d4:     READ FILE=COLVAR  IGNORE_TIME VALUES=d4
t3:     READ FILE=COLVAR  IGNORE_TIME VALUES=t3
t4:     READ FILE=COLVAR  IGNORE_TIME VALUES=t4

metad:       READ FILE=COLVAR IGNORE_TIME VALUES=metad.rbias
# Define weights
weights1: REWEIGHT_METAD TEMP=300 ARG=metad.rbias

HISTOGRAM ...

  ARG=t3,t4
  GRID_MIN=-pi,-pi
  GRID_MAX=pi,pi
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.05
  LOGWEIGHTS=weights1
  LABEL=hh12

... HISTOGRAM

HISTOGRAM ...

  ARG=t3,d1
  GRID_MIN=-pi,0.2
  GRID_MAX=pi,3.0
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.02
  LOGWEIGHTS=weights1
  LABEL=hh13

... HISTOGRAM

ff12: CONVERT_TO_FES GRID=hh12 TEMP=300 MINTOZERO
DUMPGRID GRID=ff12 FILE=fes_trpchi1chi2.dat
DUMPGRID GRID=hh12 FILE=histo_trpchi1chi2 FMT=%24.16e

ff13: CONVERT_TO_FES GRID=hh13 TEMP=300 MINTOZERO
DUMPGRID GRID=ff13 FILE=fes_trpopen.dat
DUMPGRID GRID=hh13 FILE=histo_trpopen FMT=%24.16e
