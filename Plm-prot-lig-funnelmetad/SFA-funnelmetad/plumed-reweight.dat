chi1:   READ FILE=COLVAR-driver  IGNORE_TIME VALUES=chi1_79
chi2:   READ FILE=COLVAR-driver  IGNORE_TIME VALUES=chi2_79
d1:     READ FILE=COLVAR-stride  IGNORE_TIME VALUES=d1
t3:     READ FILE=COLVAR-driver  IGNORE_TIME VALUES=chi1_43
t4:     READ FILE=COLVAR-driver  IGNORE_TIME VALUES=chi2_43
d3:      READ FILE=COLVAR-lig  IGNORE_TIME VALUES=d3
d4:      READ FILE=COLVAR-lig  IGNORE_TIME VALUES=d4

metad:       READ FILE=COLVAR-stride IGNORE_TIME VALUES=metad.rbias
uwall:       READ FILE=COLVAR-stride IGNORE_TIME VALUES=fpslpuwall.bias
lwall:       READ FILE=COLVAR-stride IGNORE_TIME VALUES=fpslplwall.bias
d1uwall:       READ FILE=COLVAR-stride IGNORE_TIME VALUES=d1uwall.bias

# Define weights
weights1: REWEIGHT_METAD TEMP=300 ARG=metad.rbias,uwall.bias,lwall.bias,d1uwall.bias

HISTOGRAM ...

  ARG=t3,t4
  GRID_MIN=-pi,-pi
  GRID_MAX=pi,pi
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.05
  LOGWEIGHTS=weights1
  LABEL=hh1

... HISTOGRAM

HISTOGRAM ...

  ARG=t3,d1
  GRID_MIN=-pi,-1.0
  GRID_MAX=pi,4.5
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.02
  LOGWEIGHTS=weights1
  LABEL=hh2

... HISTOGRAM

HISTOGRAM ...

  ARG=chi1,d1
  GRID_MIN=-pi,-1.0
  GRID_MAX=pi,4.5
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.02
  LOGWEIGHTS=weights1
  LABEL=hh3

... HISTOGRAM

HISTOGRAM ...

  ARG=t3,d3
  GRID_MIN=-pi,-1.0
  GRID_MAX=pi,5.0
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.02
  LOGWEIGHTS=weights1
  LABEL=hh4

... HISTOGRAM

HISTOGRAM ...

  ARG=d3
  GRID_MIN=-1.0
  GRID_MAX=5.0
  GRID_BIN=200
  BANDWIDTH=0.02
  LOGWEIGHTS=weights1
  LABEL=hh5

... HISTOGRAM

HISTOGRAM ...

  ARG=t3,d4
  GRID_MIN=-pi,0.2
  GRID_MAX=pi,3.0
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.02
  LOGWEIGHTS=weights1
  LABEL=hh6

... HISTOGRAM

HISTOGRAM ...

  ARG=d1
  GRID_MIN=-1.0
  GRID_MAX=4.5
  GRID_BIN=200
  BANDWIDTH=0.02
  LOGWEIGHTS=weights1
  LABEL=hh7

... HISTOGRAM

ff1: CONVERT_TO_FES GRID=hh1 TEMP=300 MINTOZERO
DUMPGRID GRID=ff1 FILE=fes_trpchi1chi2.dat
DUMPGRID GRID=hh1 FILE=histo_trpchi1chi2 FMT=%24.16e

